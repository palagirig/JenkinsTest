        <fullName>Create_Contact</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Contact</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}

// query the record
var qr = sforce.connection.query(&quot;SELECT Id, P7S1_Vorname__c, P7S1_Nachname__c, P7S1_EMail__c, P7S1_Contact__c FROM P7S1_PortalLogin__c where Id=&apos;&quot; + &quot;{!P7S1_PortalLogin__c.Id}&quot; + &quot;&apos;&quot;);
var records = qr.getArray(&quot;records&quot;);
var portalLogin = records[0];

// abort if already linked with a contact
if( portalLogin.P7S1_Contact__c != null ){
  alert(&quot;Dieser Datensatz ist bereits mit einem Kontakt verknüpft&quot;);
} else {

  // check if a contact with given email already exists
  qr = sforce.connection.query(&quot;SELECT Id, Name FROM Contact where Email =&apos;&quot; + &quot;{!P7S1_PortalLogin__c.P7S1_EMail__c}&quot; + &quot;&apos;&quot;);
  records = qr.getArray(&quot;records&quot;);
  if (records.length &gt; 0) {
    var c = records[0];

    // ask if connect to existing contact
    var r = confirm(&quot;Ein Kontakt mit dieser Email existiert bereits: &quot; + c.Name + &quot; wollen Sie ihn mit diesem Datensatz verknüpfen?&quot;);
    if (r == true) {
      portalLogin.P7S1_Contact__c = c.Id;
      sforce.connection.update([portalLogin]);
      window.location.reload();
    } else {
      ;
    }

    // forward to data trim with pre-populated fields
  } else {
    window.location = &quot;/apex/TRIMEC__DataTrim_EntryCheck?email=&quot;+portalLogin.P7S1_EMail__c+&quot;&amp;firstName=&quot;+portalLogin.P7S1_Vorname__c+&quot;&amp;lastName=&quot;+portalLogin.P7S1_Nachname__c;
  }
}</url>
    </webLinks>
