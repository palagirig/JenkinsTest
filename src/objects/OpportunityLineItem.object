        <fullName>P7S1_MassDelete</fullName>
        <availability>online</availability>
        <description>Button ermöglicht die mit Hilfe der Checkboxen ausgewählten Produkte zu löschen.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Löschen</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/connection.js&quot;)}

var records = {!GETRECORDIDS( $ObjectType.OpportunityLineItem )};

if (records[0] == null) {
alert(&quot;Bitte mindestens ein Produkt auswählen.&quot;) }
else {

