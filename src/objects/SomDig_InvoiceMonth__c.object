    <webLinks>
        <fullName>SomDig_InvoiceMonth_CloseMonth</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Monat schließen</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)}
debugger;

try {
	sforce.apex.execute(&quot;SomDig_InvoiceMonth_CloseMonth&quot;,&quot;closeMonthValidation&quot;, {invoiceMonth:&quot;{!SomDig_InvoiceMonth__c.Id}&quot;});
	if(confirm(&quot;Sind Sie sicher, dass Sie den Monat (für die Faktura) schließen möchten?&quot;)) {
		sforce.apex.execute(&quot;SomDig_InvoiceMonth_CloseMonth&quot;,&quot;closeMonth&quot;,{invoiceMonth:&quot;{!SomDig_InvoiceMonth__c.Id}&quot;});
	}
	window.location.reload();
}
catch(e) {
	var error = /{.*?faultstring:&apos;.*?: (.*?)\n*.*?\n*}/.exec(e);
	if (error != null) {
		alert(error[1]);
	}
	else {
		alert(e);
	} 
}</url>
