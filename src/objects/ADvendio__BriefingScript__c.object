    <webLinks>
        <fullName>ADvendio__SendEmailToContact</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send Email To Contact</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/apex.js&quot;)}
var result = sforce.connection.query(&quot;Select Id,Name,ADvendio__Status__c, ADvendio__SentToClientScript__c,ADvendio__SentToClientAd__c From ADvendio__BriefingScript__c where Id=&quot;+&quot;&apos;{!ADvendio__BriefingScript__c.Id}&apos;&quot;);

var records = result.getArray(&quot;records&quot;);

if (records.length != 0){
    var scriptStatus = records[0].ADvendio__Status__c;
    var scriptEmailSent = records[0].ADvendio__SentToClientScript__c;
    var adEmailSent = records[0].ADvendio__SentToClientAd__c;
    if((scriptStatus == &apos;Script Pending Client approval&apos; &amp;&amp; scriptEmailSent ==  &apos;true&apos;) || (scriptStatus == &apos;Advertise Pending Client approval&apos; &amp;&amp; adEmailSent  == &apos;true&apos;)){
        alert(&apos;The script is already Sent to client&apos;);
    }
    else if((scriptStatus == &apos;Script Pending Client approval&apos; &amp;&amp; scriptEmailSent == &apos;false&apos;) || (scriptStatus == &apos;Advertise Pending Client approval&apos; &amp;&amp; adEmailSent  ==&apos;false&apos;)){
        var url = &apos;/apex/ADvendio__MMScriptAdApprovalEmailToContact?Id={!ADvendio__BriefingScript__c.Id}&apos;;
        window.open(url);
    }else{
        alert(&apos;Script/AD Status should be Script/AD Pending Client approval to send PDF to contact&apos;);
    }
}</url>
    </webLinks>
