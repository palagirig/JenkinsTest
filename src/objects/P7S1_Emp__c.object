        <fullName>P7S1_CompanyCheck</fullName>
        <active>true</active>
        <description>Checks that Frima is assigned and Firma(Text) is empty before syncing.</description>
        <errorConditionFormula>AND(
 P7S1_Sync__c,
 OR(   
  P7S1_Comp__c = $Setup.P7S1_Settings__c.P7S1_Emp_Serviceweb_DefaultCompID__c,
  NOT(ISBLANK( P7S1_CompText__c ))
 )
 
)</errorConditionFormula>
