    <fields>
        <fullName>SomDig_AnteilCommitmentsAJ__c</fullName>
        <description>zeigt Anteil des Commitments im AJ</description>
        <externalId>false</externalId>
        <formula>IF(AND(YEAR(TODAY()) = YEAR (SomDig_GueltigVon__c ), YEAR(TODAY()) = YEAR (SomDig_GueltigBis__c )
), SomDig_UmsatzGesamtMN2__c,
IF(AND(YEAR(TODAY())-1 = YEAR (SomDig_GueltigVon__c ), YEAR(TODAY()) = YEAR (SomDig_GueltigBis__c )
), SomDig_UmsatzGesamtMN2__c*(SomDig_GueltigBis__c - DATE(YEAR(TODAY()),1,1) + 1)/(SomDig_GueltigBis__c-SomDig_GueltigVon__c+1),  
IF(AND(YEAR(TODAY()) = YEAR (SomDig_GueltigVon__c ), YEAR(TODAY())+1 = YEAR (SomDig_GueltigBis__c )
), SomDig_UmsatzGesamtMN2__c*(DATE(YEAR(TODAY()),12,31)-SomDig_GueltigVon__c+ 1)/(SomDig_GueltigBis__c-SomDig_GueltigVon__c+1),
0)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>zeigt Anteil des Commitments im AJ</inlineHelpText>
        <label>Anteil der Commitments im AJ</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
