{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}
window.requestApproval_waiting = window.requestApproval_waiting || false;
console.log(window.requestApproval_waiting);
if(!window.requestApproval_waiting){
    var callback = {
        onSuccess: function(result){
            alert(result);
            window.requestApproval_waiting = false;
            window.location.reload();
        },
        onFailure: function(error){
            if(error.faultstring.includes(&quot;ALREADY_IN_PROCESS&quot;)){
                alert(&quot;This record is currently in an approval process. A record can be in only one approval process at a time&quot;);
            }else{
            alert(error.faultstring);
            }

            window.requestApproval_waiting = false;
        }
    };
    sforce.apex.execute(&quot;MT_ApprovalProcessModule&quot;,&quot;startApprovalProcess&quot;,{strOppAppId:&quot;{!MT_MediaCampaign__c.MT_OpportunityApproval_refId__c}&quot;,strCampId : &quot;{!MT_MediaCampaign__c.Id}&quot;},callback);
    window.requestApproval_waiting = true;
