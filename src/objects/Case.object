    <validationRules>
        <fullName>Lcm_BlockReopenTicket</fullName>
        <active>true</active>
        <errorConditionFormula>$Profile.Id != $Label.ProfileIdAdmin
&amp;&amp; $User.Id != &apos;005D0000001WzTPIA0&apos; 
&amp;&amp; (IsClosed = true || PRIORVALUE(IsClosed) = true)
&amp;&amp; NOT (ISCHANGED(  P7S1_ExtDueDate__c ) )
&amp;&amp; NOT (ISCHANGED(  P7S1_ExtPrio__c ) )
&amp;&amp; NOT (ISCHANGED(  externer_Link__c ) )
&amp;&amp; NOT (ISCHANGED(  P7S1_ExtProjektmanager__c ) )
&amp;&amp; NOT (ISCHANGED(  P7S1_ExtState__c ) )
&amp;&amp; NOT (ISCHANGED(  P7S1_ExtCaseNo__c ) )
&amp;&amp; NOT (ISCHANGED(  P7S1_LastCaseComment__c ) )</errorConditionFormula>
        <errorMessage>Geschlossenes Ticket kann nicht geöffnet oder geändert werden.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>P7S1_AdvProjektAdmin</fullName>
        <active>false</active>
        <description>Es dürfen durch Nicht-Admins keine Tickets in den Projekten 
Adv Betrieb 
Adv Delta Force 
Adv Features 
Adv Modularisierung 
angelegt werden.</description>
        <errorConditionFormula>AND ( 
$User.ProfileId &lt;&gt; $Label.ProfileIdAdmin , 
OR (
ISNEW() ,
ISCHANGED( Projekt__c )
) ,
CASE (TEXT( Projekt__c ), 
&apos;Adv Sonstiges&apos; , 0 , 
&apos;Adv Betrieb&apos; , 1 , 
&apos;Adv Delta Force&apos; , 1 , 
&apos;Adv Features&apos; , 1 , 
&apos;Adv Modularisierung&apos; , 1 , 
0) = 1 
)</errorConditionFormula>
        <errorDisplayField>Projekt__c</errorDisplayField>
        <errorMessage>Es können nur Tickets für das Projekt &quot;Adv Sonstiges&quot; angelegt werden. Die Zuordnung zu den anderen Adv Projekten erfolgt im Refinement!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rank_Dublette</fullName>
        <active>false</active>
        <description>Dubletten im Feld &apos;Rank per Projekt&apos; vermeiden.</description>
        <errorConditionFormula>NOT(ISBLANK((Text( Projekt__c) &amp; &quot; &quot; &amp; Text(Rank__c))))</errorConditionFormula>
        <errorDisplayField>Rank__c</errorDisplayField>
        <errorMessage>Dublette für diesen Rank im jeweiligen Projekt, bitte einen anderen Rank auswählen.</errorMessage>
    </validationRules>
