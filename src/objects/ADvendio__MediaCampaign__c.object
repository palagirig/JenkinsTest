    </validationRules>
    <validationRules>
        <fullName>SomDig_MKCheck3</fullName>
        <active>false</active>
        <description>Die Phase kann nicht geändert werden, wenn die MK nicht genehmigt ist. Bitte starten sie einen Genehmigungsprozess.</description>
        <errorConditionFormula>NOT(ISPICKVAL(ADvendio__Stage__c,&quot;First contact&quot;))
&amp;&amp;
