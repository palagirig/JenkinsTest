        <formula>CASE(1, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(5%)&quot;), TEXT(ADvendio__Stage__c)= &quot;First contact&quot;), 1, 0), 0.05, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(10%)&quot;), TEXT(ADvendio__Stage__c)= &quot;Initial Offer (w/o request)&quot;), 1, 0), 0.1, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(20%)&quot;),TEXT(ADvendio__Stage__c)= &quot;Stage 20%&quot;), 1, 0), 0.2, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(25%)&quot;),TEXT(ADvendio__Stage__c)= &quot;Stage 25%&quot;), 1, 0), 0.25, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(30%)&quot;),TEXT(ADvendio__Stage__c)= &quot;Alternative quote&quot;), 1, 0), 0.3, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(40%)&quot;), TEXT(ADvendio__Stage__c)= &quot;Stage 40%&quot;), 1, 0), 0.4, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(50%)&quot;), TEXT(ADvendio__Stage__c)= &quot;Offer on Request&quot;), 1, 0), 0.5, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(60%)&quot;), TEXT(ADvendio__Stage__c)= &quot;Stage 60%&quot;), 1, 0), 0.6, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(70%)&quot;), TEXT(ADvendio__Stage__c)= &quot;Showed interest&quot;), 1, 0), 0.7, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(75%)&quot;), TEXT(ADvendio__Stage__c)= &quot;Stage 75%&quot;), 1, 0), 0.75, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(80%)&quot;), TEXT(ADvendio__Stage__c)= &quot;Positive Feedback (Agency/Advertiser)&quot;), 1, 0), 0.8, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(90%)&quot;), TEXT(ADvendio__Stage__c)= &quot;Stage 90%&quot;), 1, 0), 0.9, IF(OR(CONTAINS(TEXT(ADvendio__Stage__c), &quot;(100%)&quot;), TEXT(ADvendio__Stage__c)= &quot;Booked&quot;, TEXT(ADvendio__Stage__c)= &quot;IO received&quot;), 1, 0), 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
