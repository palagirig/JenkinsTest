{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/apex.js&quot;)} 


var listCI = sforce.connection.query(&quot;SELECT ID FROM ADvendio__Campaign_Item__c WHERE (SomDig_PositionStatus__c = &apos;Ad Status aktualisieren&apos; OR SomDig_PositionStatus__c = &apos;An Adserver √ºbermitteln&apos;) AND ADvendio__Media_Campaign__c = &apos;{!ADvendio__MediaCampaign__c.Id}&apos;&quot;); 

var iSize = listCI.getArray(&quot;records&quot;).length; 



var listCINotStopped = sforce.connection.query(&quot;SELECT ID FROM ADvendio__Campaign_Item__c WHERE SomDig_PositionStatus__c != &apos;Position gestoppt&apos; AND SomDig_PositionStatus__c != &apos;NonAdserver&apos; AND ADvendio__Media_Campaign__c = &apos;{!ADvendio__MediaCampaign__c.Id}&apos;&quot;); 

var iSize2 = listCINotStopped.getArray(&quot;records&quot;).length; 




/*KV 2787 */ 
var listCIAdServerRel = sforce.connection.query(&quot;SELECT ADvendio__LastAvailabilityForecast__c, ADvendio__Availability__c FROM ADvendio__Campaign_Item__c WHERE RecordType.DeveloperName != &apos;Packet&apos; AND SomDig_AdServerRelevant__c = true AND ADvendio__Media_Campaign__c = &apos;{!ADvendio__MediaCampaign__c.Id}&apos; AND ADvendio__Media_Campaign__r.SomDig_Verfuegbarkeitsrelevant__c = true&quot;); 
var records = listCIAdServerRel.getArray(&quot;records&quot;); 

var dateNow = new Date(); 
var availabilityChecked = true; 


for (var i=0; i&lt; records.length; i++) { 

if(records[i].ADvendio__LastAvailabilityForecast__c == null){ 
availabilityChecked = false; 
}else{ 
var dateLastforCast = new Date(records[i].ADvendio__LastAvailabilityForecast__c); 
var iMinutesPassed = (dateNow - dateLastforCast) / 60000; 
if(iMinutesPassed &gt; 180) availabilityChecked = false; 

if(records[i].ADvendio__Availability__c == null) availabilityChecked = false; 
} 
} 






if(&quot;{!ADvendio__MediaCampaign__c.RecordType}&quot; == &quot;Auftrag&quot; &amp;&amp; iSize &gt; 0){ 
alert(&quot;Dokument kann nicht erstellt werden, da mindestens bei einer Position der Status auf &apos;Ad Status aktualisieren&apos; oder &apos;An Adserver √ºbermitteln&apos; steht&quot;); 
}else if(availabilityChecked == false &amp;&amp; &quot;{!ADvendio__MediaCampaign__c.RecordType}&quot; == &quot;Angebot&quot;){ 
/*KV 2787*/ 
alert(&quot;Die Verf√ºgbarkeit wurde innerhalb der letzten 3h noch nicht durchgef√ºhrt.&quot;); 

}else if( &quot;{!ADvendio__MediaCampaign__c.ADvendio__Stage__c}&quot; == &quot;Offer denied&quot; &amp;&amp; &quot;{!ADvendio__MediaCampaign__c.ADvendio__Reason_for_refusal__c}&quot; == &quot;Cancellation&quot; &amp;&amp; iSize2 &gt; 0){ 
alert(&quot;Bitte erst den Storno der Mediakampagne an den AdServer √ºbergeben via Button &apos;Ad Status aktualisieren&apos;. Danach kann das Stornodokument erstellt werden.&quot;); 
}else{ 
window.open(&quot;/apex/ADvendio__CheckQuotePreference?id={!ADvendio__MediaCampaign__c.Id}&quot;, &quot;Auftragsbest√§tigung&quot;, &quot;fullscreen=yes,status=yes,scrollbars=yes,resizable=yes&quot;); 
}</url>
