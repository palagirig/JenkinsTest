    <validationRules>
        <fullName>SomDig_Abrechnen_KeinStandard</fullName>
        <active>true</active>
        <description>#04209 - Befindet sich eine Mediakampagne (welche keine Gutschrift ist) im Angebotsstatus und hat nicht den Auftragstyp (in der Preisliste zu finden) &quot;Standard&quot;, darf der Wert &quot;Abrechnen&quot; nur von einem Benutzer mit dem Billing-, CAM-, BI-, Sales geändert</description>
        <errorConditionFormula>/* 012D0000000Qj96 = Angebot */ 
ISCHANGED( ADvendio__BillMe__c ) 
&amp;&amp; RecordTypeId = &apos;012D0000000Qj96&apos; 
&amp;&amp; ADvendio__Pricelist__r.SomDig_AuftragstypId__r.Name != &apos;Standard&apos; 
&amp;&amp; SomDig_GutschriftPerformance__c = false 
&amp;&amp; ( $Profile.Id != &apos;00e20000000lfOj&apos; 
&amp;&amp; $Profile.Id != &apos;00eD0000001Qk4n&apos; 
&amp;&amp; $Profile.Id != &apos;00eD0000001Qf2o&apos; 
