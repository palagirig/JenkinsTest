    <fields>
        <fullName>ADvendio__InvoiceQuantity__c</fullName>
        <deprecated>false</deprecated>
        <description>The quantity which is invoiced and might be used for publisher payout.</description>
        <externalId>false</externalId>
        <formula>CASE (
ADvendio__CampaignItem__r.ADvendio__Billing_category__c,

&quot;CPM&quot;,
IF( NOT(ISBLANK(ADvendio__DeliveredAIsManual__c)), ADvendio__DeliveredAIsManual__c,
IF( NOT(ISBLANK(ADvendio__DeliveredImpressions__c)), ADvendio__DeliveredImpressions__c, 0)),

&quot;CPC&quot;,
IF( NOT(ISBLANK(ADvendio__DeliveredClicksManual__c)), ADvendio__DeliveredClicksManual__c,
IF( NOT(ISBLANK(ADvendio__DeliveredClicks__c)), ADvendio__DeliveredClicks__c, 0)),

&quot;vCPM&quot;,
IF( NOT(ISBLANK(ADvendio__ViewedAIsManual__c)), ADvendio__ViewedAIsManual__c,
IF( NOT(ISBLANK(ADvendio__ViewedAIs__c)), ADvendio__ViewedAIs__c, 0)),0
)</formula>
        <inlineHelpText>Displays the quantity which will be used for invoicing, based upon the Billing Category of the Campaign Item.</inlineHelpText>
        <label>Invoice Quantity</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
