    <webLinks>
        <fullName>ADvendio__AdFillReport</fullName>
        <availability>online</availability>
        <description>Links directly to the Ad Fill report, to have an overview over the planning status of your Publication Dates.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Ad fill Report</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)}

try {
var result = sforce.connection.query(
&quot;SELECT Id&quot; +
&quot; FROM Report&quot; +
&quot; WHERE DeveloperName = &apos;Adfill_Report_per_Publication_date&apos;&quot; +
&quot; AND NamespacePrefix = &apos;ADvendio&apos;&quot; +
&quot; LIMIT 1&quot;
);

var reports = result.getArray(&quot;records&quot;);

if (reports.length === 0) {
alert(&quot;The Report isn&apos;t available in this Org!&quot;);
} else {
var url = &quot;/&quot; + reports[0].Id + &quot;?pv0={!ADvendio__PublicationDate__c.Id}&quot;;
window.open(url, &quot;_blank&quot;);
}
} catch(e) {
alert(
&quot;An unexpected Error has Occurred.\r\n&quot; + 
&quot;Error: &quot; + e.message
);
}</url>
    </webLinks>
