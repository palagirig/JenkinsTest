        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 

var records = [&apos;{!ADvendio__Invoice__c.Id}&apos;];
try { 
  sforce.apex.execute(&quot;SomDig_Rechnungsfreigabe&quot;,&quot;starteRechungsfreigabe&quot;,{invoiceIds : records}); 
alert(&quot;Die Rechnungsfreigabe für die ausgewählten Rechnungen wurde gestartet! Sie werden per Email benachrichtigt wenn der Prozess beendet ist.&quot; ); 
} 
catch(e) { 
  var error = /faultstring:&apos;.*?: (.*?)\n([\s\S]*)/.exec(e); 
  if (error != null) { 
    alert(error[1]); 
  } 
  else { 
    alert(e); 
  } 
}</url>
