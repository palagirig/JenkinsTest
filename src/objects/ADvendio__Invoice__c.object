    <validationRules>
        <fullName>Beschreibungsfeld</fullName>
        <active>true</active>
        <description>Beschreibung darf nicht geändert werden, wenn Rechnung im Fakturiert- oder Storniert-Status ist (siehe auch KV 2924)</description>
        <errorConditionFormula>ISCHANGED( ADvendio__Description__c ) 
 &amp;&amp; 
OR(
ISPICKVAL( ADvendio__Status__c , &quot;Invoiced&quot;),
ISPICKVAL( ADvendio__Status__c , &quot;Canceled&quot;))</errorConditionFormula>
        <errorMessage>Beschreibung darf nicht geändert werden, wenn Rechnung im Fakturiert- oder Storniert-Status ist</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SomDig_CanceledMustNeverChanged</fullName>
        <active>true</active>
        <errorConditionFormula>isCHanged(ADvendio__Status__c) &amp;&amp; 
IsPickVal(PRIORVALUE(ADvendio__Status__c),&quot;Canceled&quot;)</errorConditionFormula>
        <errorDisplayField>ADvendio__Status__c</errorDisplayField>
        <errorMessage>Eine Rechnung im Status &quot;Storniert&quot; darf in keinen anderen Status geändert werden</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SomDig_FreigabeBlock</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(SomDig_Rechnungsfreigabe__c)
&amp;&amp;
OR(
ISPICKVAL(ADvendio__Status__c,&quot;Invoiced&quot;),
ISPICKVAL(ADvendio__Status__c,&quot;Canceled&quot;)
)</errorConditionFormula>
        <errorMessage>Entfernen der Freigabe in diesem Status nicht möglich.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SomDig_FreigabeCheck</fullName>
        <active>true</active>
        <description>Ticket 04245</description>
        <errorConditionFormula>ISBLANK(ADvendio__Invoice__c) &amp;&amp;
(NOT(ISPICKVAL(PRIORVALUE(ADvendio__Status__c), &quot;Reviewed&quot;)) &amp;&amp; ISPICKVAL(ADvendio__Status__c, &quot;Reviewed&quot;) ||
PRIORVALUE(SomDig_Rechnungsfreigabe__c) = FALSE &amp;&amp; SomDig_Rechnungsfreigabe__c = TRUE) &amp;&amp;
ISPICKVAL(ADvendio__MediaCampaign__r.ADvendio__invoice_recipient__r.P7S1_Steuerkennzeichen__c, &quot;AE&quot;) &amp;&amp;
ISBLANK( ADvendio__MediaCampaign__r.ADvendio__invoice_recipient__r.P7S1_UstIdNr__c )</errorConditionFormula>
        <errorMessage>Die Rechnung kann nicht freigegeben werden, da keine USt-ID am Account hinterlegt wurde. Bitte nachtragen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SomDig_InvoicedMustOnlyChangeToCanceled</fullName>
        <active>true</active>
        <errorConditionFormula>isCHanged(ADvendio__Status__c) &amp;&amp; isPickVal(PRIORVALUE(ADvendio__Status__c), &quot;Invoiced&quot;) &amp;&amp; 
Not(isPickVal(ADvendio__Status__c, &quot;Canceled&quot;))</errorConditionFormula>
        <errorDisplayField>ADvendio__Status__c</errorDisplayField>
        <errorMessage>Eine Rechnung im Status &quot;Fakturiert&quot; darf nur in den Status &quot;Storniert&quot; geändert werden</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SomDig_NullEuroRechnung</fullName>
        <active>true</active>
        <description>Ticket 03828. Ein Rechnung mit einem MN2 von 0€ darf nicht freigegeben werden.</description>
        <errorConditionFormula>SomDig_Rechnungsfreigabe__c = true &amp;&amp; PRIORVALUE(SomDig_Rechnungsfreigabe__c) = false &amp;&amp; ADvendio__InvoicedAmountNet2__c = 0</errorConditionFormula>
        <errorMessage>Ein Rechnung mit einem MN2 von 0€ darf nicht freigegeben werden.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SomDig_RechnungsnummerFixieren</fullName>
        <active>true</active>
        <description>Die Rechnungsnummer einer fakturierten Rechnung darf nicht geändert werden.</description>
        <errorConditionFormula>ISCHANGED(Name) &amp;&amp; LEFT( PRIORVALUE(Name), 3) == &quot;420&quot;</errorConditionFormula>
        <errorMessage>Die Rechnungsnummer einer fakturierten Rechnung darf nicht geändert werden!</errorMessage>
    </validationRules>
