    <fields>
        <fullName>ADvendio__InvoiceQuantityForecast__c</fullName>
        <deprecated>false</deprecated>
        <description>This field calculates the invoice quantity forecast depending on linear distribution, start of the campaign item and month.</description>
        <externalId>false</externalId>
        <formula>IF (
	ADvendio__Campaign_Item__r.ADvendio__LinearDistribution__c || ADvendio__Campaign_Item__r.ADvendio__from_Date__c &gt; TODAY() ,
	ADvendio__Campaign_Item__r.ADvendio__Quantity__c / (
		(
			IF(
				YEAR(ADvendio__Campaign_Item__r.ADvendio__until_Date__c) - YEAR(ADvendio__Campaign_Item__r.ADvendio__from_Date__c) &gt; 1,
				YEAR(ADvendio__Campaign_Item__r.ADvendio__until_Date__c) - YEAR(ADvendio__Campaign_Item__r.ADvendio__from_Date__c) - 1,
				0
			)
		) * 12
		+
		(
		IF(
			YEAR(ADvendio__Campaign_Item__r.ADvendio__from_Date__c) &lt; YEAR(ADvendio__Campaign_Item__r.ADvendio__until_Date__c),
			12 - MONTH(ADvendio__Campaign_Item__r.ADvendio__from_Date__c) + MONTH(ADvendio__Campaign_Item__r.ADvendio__until_Date__c),
			MONTH(ADvendio__Campaign_Item__r.ADvendio__until_Date__c) - MONTH(ADvendio__Campaign_Item__r.ADvendio__from_Date__c)
		) + 1)
	)
, 
	IF (
		ADvendio__Month__c &lt; TODAY(),
		ADvendio__InvoiceQuantity__c, 
		IF (
			ADvendio__AccumulatedDeliveryTillNow__c = 0,
			0,
			IF (
				ADvendio__Campaign_Item__r.ADvendio__Quantity__c - ADvendio__AccumulatedDeliveryTillNow__c &gt; 0, 
				(ADvendio__Campaign_Item__r.ADvendio__Quantity__c - ADvendio__AccumulatedDeliveryTillNow__c) / (MONTH(ADvendio__Campaign_Item__r.ADvendio__until_Date__c) - MONTH(TODAY())),
				0
			)
		)
	) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This field calculates the invoice quantity forecast depending on linear distribution, start of the campaign item and month.</inlineHelpText>
        <label>Invoice Quantity Forecast</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
