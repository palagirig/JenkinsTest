    <fields>
        <fullName>ADvendio__InvoiceQuantity__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF (
	ADvendio__Campaign_Item__r.ADvendio__LinearDistribution__c,
	ADvendio__Campaign_Item__r.ADvendio__Quantity__c / ADvendio__Campaign_Item__r.ADvendio__Days__c * ADvendio__Days__c,
	IF(
		!ISBLANK(ADvendio__QuantityManualFormula__c),
		MAX(0, MIN(ADvendio__Campaign_Item__r.ADvendio__Quantity__c - ADvendio__AccumulatedDeliveryTillNow__c + ADvendio__QuantityManualFormula__c, ADvendio__QuantityManualFormula__c)),
		

		CASE (
			ADvendio__Campaign_Item__r.ADvendio__Billing_category__c,
			
			&quot;CPM&quot;,
			IF(
				ISBLANK(ADvendio__Delivered_AIs__c),
				0,
				MAX(0, MIN(ADvendio__Campaign_Item__r.ADvendio__Quantity__c - ADvendio__AccumulatedDeliveryTillNow__c + ADvendio__Delivered_AIs__c, ADvendio__Delivered_AIs__c))),
			
			&quot;CPC&quot;,
			IF(
				ISBLANK(ADvendio__Delivered_Clicks__c),
				0,
				MAX(0, MIN(ADvendio__Campaign_Item__r.ADvendio__Quantity__c - ADvendio__AccumulatedDeliveryTillNow__c + ADvendio__Delivered_Clicks__c, ADvendio__Delivered_Clicks__c))),
			
			&quot;vCPM&quot;,
			IF(
				ISBLANK(ADvendio__ViewedAIs__c),
				0,
				MAX(0, MIN(ADvendio__Campaign_Item__r.ADvendio__Quantity__c - ADvendio__AccumulatedDeliveryTillNow__c + ADvendio__ViewedAIs__c, ADvendio__ViewedAIs__c))),
			0
			 )
	)
)</formula>
        <label>Invoice Quantity (RS)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
