    <fields>
        <fullName>ADvendio__InvoiceAmountForecast__c</fullName>
        <deprecated>false</deprecated>
        <description>This field calculates the invoice amount forecast depending on linear distribution, start of the campaign item and month.</description>
        <externalId>false</externalId>
        <formula>CASE(
	1, 
	IF(
		ADvendio__Campaign_Item__r.ADvendio__LinearDistribution__c = TRUE || ADvendio__Campaign_Item__r.ADvendio__from_Date__c &gt; TODAY(),
		1,
		0
	), 
	ADvendio__Campaign_Item__r.ADvendio__Amount_net3APEX__c / (
		(
			IF(
				YEAR(ADvendio__Campaign_Item__r.ADvendio__until_Date__c) - YEAR(ADvendio__Campaign_Item__r.ADvendio__from_Date__c) &gt; 1,
				YEAR(ADvendio__Campaign_Item__r.ADvendio__until_Date__c) - YEAR(ADvendio__Campaign_Item__r.ADvendio__from_Date__c) - 1,
				0
			)
		) * 12
		+
		(
			IF(
				YEAR(ADvendio__Campaign_Item__r.ADvendio__from_Date__c) &lt; YEAR(ADvendio__Campaign_Item__r.ADvendio__until_Date__c),
				12 - MONTH(ADvendio__Campaign_Item__r.ADvendio__from_Date__c) + MONTH(ADvendio__Campaign_Item__r.ADvendio__until_Date__c),
				MONTH(ADvendio__Campaign_Item__r.ADvendio__until_Date__c) - MONTH(ADvendio__Campaign_Item__r.ADvendio__from_Date__c)
			) + 1
		)
	), 
	ADvendio__Campaign_Item__r.ADvendio__Amount_net3APEX__c * 
	(
		ADvendio__InvoiceQuantityForecast__c / ADvendio__Campaign_Item__r.ADvendio__Quantity__c
	) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This field calculates the invoice amount forecast depending on linear distribution, start of the campaign item and month.</inlineHelpText>
        <label>Invoice Amount Forecast</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
