        <externalId>false</externalId>
        <formula>IF( OR (LastName =&quot;NN&quot;, ISPICKVAL(Salutation,&apos;&apos;)), &quot;Sehr geehrte Damen und Herren&quot;, 
CASE( Salutation , 
&quot;Herr&quot;, &quot;Sehr geehrter Herr&quot;, 
&quot;Frau&quot;, &quot;Sehr geehrte Frau&quot; , 
&quot;Mr.&quot;, &quot;Sehr geehrter Herr&quot;, 
&quot;Mrs.&quot;, &quot;Sehr geehrte Frau&quot;, 
&quot;&quot;) 
&amp;
if (ISPICKVAL(SomTv_TitelAkademischerGrad__c , &apos;&apos;) , &apos;&apos; , (&quot; &quot; &amp; Text(SomTv_TitelAkademischerGrad__c)) )
&amp;
if (ISPICKVAL(SomTv_AdelsTitel__c, &apos; &apos;) ,  &apos;&apos; ,  (&quot; &quot; &amp; TEXT( SomTv_AdelsTitel__c )) )
&amp; LastName 
)</formula>
