        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} //adds the proper code for inclusion of AJAX toolkit
var contactId= &quot;{!Contact.Id}&quot;;
var updateArray = []; //Array to hold the updated Contact
var url = parent.location.href; //string for the URL of the current page

//fetch data from the Opportunity
var result = sforce.connection.query(&quot;SELECT Id, Account.BillingStreet, Account.BillingPostalCode, Account.BillingCity, Account.BillingState, Account.BillingCountry FROM Contact where Id = &apos;&quot; + contactId + &quot;&apos;&quot;).getArray(&quot;records&quot;);

//create new Contact, assign ID from grabbed record and fill it with data from the Account
var contact = new sforce.SObject(&quot;Contact&quot;);
var acc = result[0].Account;
contact.Id = contactId;
//from the Account field of the Contact:
contact.MailingStreet = acc.BillingStreet;
contact.MailingPostalCode = acc.BillingPostalCode;
contact.MailingCity = acc.BillingCity;
contact.MailingState = acc.BillingState;
contact.MailingCountry = acc.BillingCountry;

updateArray.push(contact);

//update Contact
var result = sforce.connection.update(updateArray);
if (result[0].success==&apos;false&apos;){
alert(result[0].errors.message);
}
else{
parent.location.href = url; //refresh the page
}</url>
