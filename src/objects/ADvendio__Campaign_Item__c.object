    <fields>
        <fullName>ADvendio__PerformanceIndicator__c</fullName>
        <deprecated>false</deprecated>
        <description>This field shows the current delivery status as a percentage.</description>
        <externalId>false</externalId>
        <formula>CASE ( /* Billing Category */
	1,
	IF ( /* use AI */
		TEXT(ADvendio__Billing_category__c) == &quot;CPM&quot; ||
		TEXT(ADvendio__Billing_category__c) == &quot;CPD&quot; ||
		TEXT(ADvendio__Billing_category__c) == &quot;CPW&quot; ||
		TEXT(ADvendio__Billing_category__c) == &quot;CPMo&quot; ||
		TEXT(ADvendio__Billing_category__c) == &quot;Fixed Price&quot;,
		1,
		0
	),

	CASE ( /* Runtime */
 		1,
		IF ( /* running... */
			ADvendio__from_Date__c &lt; TODAY() &amp;&amp;
			ADvendio__until_Date__c &gt;= TODAY() &amp;&amp;
			ADvendio__Delivered_Impressions__c &gt; 0,
			1,
			0
		),
		BLANKVALUE(ADvendio__Delivered_Impressions__c, 0) +
		(
			(
				BLANKVALUE(ADvendio__Delivered_Impressions__c, 0) /
				MAX(TODAY() - ADvendio__from_Date__c,1) *
				(ADvendio__until_Date__c - TODAY() +1)
			)
		),

		IF( /* ended... */
			ADvendio__until_Date__c &lt; TODAY() &amp;&amp;
			ADvendio__Delivered_Impressions__c &gt; 0,
			1,
			0
		),
		BLANKVALUE(ADvendio__Delivered_Impressions__c, 0),

		0
	),

	IF ( /* use clicks */
		TEXT(ADvendio__Billing_category__c) == &quot;CPC&quot;,
		1,
		0
	),

	CASE ( /* Runtime */
 		1,
		IF ( /* running... */
			ADvendio__from_Date__c &lt; TODAY() &amp;&amp;
			ADvendio__until_Date__c &gt;= TODAY() &amp;&amp;
			ADvendio__Delivered_Clicks__c &gt; 0,
			1,
			0
		),
		BLANKVALUE(ADvendio__Delivered_Clicks__c, 0) +
		(
			(
				BLANKVALUE(ADvendio__Delivered_Clicks__c, 0) /
				MAX(TODAY() - ADvendio__from_Date__c,1) *
				(ADvendio__until_Date__c - TODAY() +1)
			)
		),

		IF( /* ended... */
			ADvendio__until_Date__c &lt; TODAY() &amp;&amp;
			ADvendio__Delivered_Clicks__c &gt; 0,
			1,
			0
		),
		BLANKVALUE(ADvendio__Delivered_Clicks__c, 0),
		
		0
	),

	IF ( /* use visible AI */
		TEXT(ADvendio__Billing_category__c) == &quot;vCPM&quot;,
		1,
		0
	),

	CASE ( /* Runtime */
 		1,
		IF ( /* running... */
			ADvendio__from_Date__c &lt; TODAY() &amp;&amp;
			ADvendio__until_Date__c &gt;= TODAY() &amp;&amp;
			ADvendio__AvAsViewableAi__c &gt; 0,
			1,
			0
		),
		BLANKVALUE(ADvendio__AvAsViewableAi__c, 0) +
		(
			(
				BLANKVALUE(ADvendio__AvAsViewableAi__c, 0) /
				MAX(TODAY() - ADvendio__from_Date__c,1) *
				(ADvendio__until_Date__c - TODAY() + 1)
			)
		),

		IF( /* ended... */
			ADvendio__until_Date__c &lt; TODAY() &amp;&amp;
			ADvendio__AvAsViewableAi__c &gt; 0,
			1,
			0
		),
		BLANKVALUE(ADvendio__AvAsViewableAi__c, 0),
		0
	),
	0
) / ADvendio__BookingQuantity__c</formula>
