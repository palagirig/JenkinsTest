  /* Auftrag */
  (
    $Profile.Name = &apos;SomDig_Billing&apos;
    ||
    (
      $Profile.Name = &apos;SomDig_BI&apos;
      &amp;&amp;
      (
        LEFT(SomDig_Auftragstyp__c,15) = &apos;a34D0000000UJcQ&apos; /* AdAudience */
        ||
        LEFT(SomDig_Auftragstyp__c,15) = &apos;a34D0000000UJcN&apos; /* Performance */
      )
    )
