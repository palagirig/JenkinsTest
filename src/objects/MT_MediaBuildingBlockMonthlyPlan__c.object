    <validationRules>
        <fullName>MT_BMV_Sponsoring_MUST_BE_POSITIVE</fullName>
        <active>true</active>
        <errorConditionFormula>MT_BMVSponsoring__c &lt; 0</errorConditionFormula>
        <errorDisplayField>MT_BMVSponsoring__c</errorDisplayField>
        <errorMessage>BMV Sponsoring cannot be less then 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_BMW_Spot_MUST_BE_POSSITIVE</fullName>
        <active>true</active>
        <errorConditionFormula>MT_BMVSpot__c &lt; 0</errorConditionFormula>
        <errorDisplayField>MT_BMVSpot__c</errorDisplayField>
        <errorMessage>BMV Spot cannot be less than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_FIX_Revenue_MUST_BE_POSITIVE</fullName>
        <active>true</active>
        <errorConditionFormula>MT_FixRevenue__c &lt; 0</errorConditionFormula>
        <errorDisplayField>MT_FixRevenue__c</errorDisplayField>
        <errorMessage>Fix Revenue cannot be less than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_IsMonthInContractRuntime</fullName>
        <active>true</active>
        <description>Determines if the Month is in between the Start and End Date of this Contract (defined on the opportunity approval)</description>
        <errorConditionFormula>OR(
  DATE(YEAR(MT_MediaBuildingBlock_ref__r.MT_MediaCampaign_ref__r.MT_Sys_ContractOrTVRuntimeStart__c),MONTH(MT_MediaBuildingBlock_ref__r.MT_MediaCampaign_ref__r.MT_Sys_ContractOrTVRuntimeStart__c),1) &gt; DATE(YEAR(MT_PlanningDate__c),MONTH(MT_PlanningDate__c),1),
  DATE(YEAR(MT_MediaBuildingBlock_ref__r.MT_MediaCampaign_ref__r.MT_Sys_ContractOrTVRuntimeEnd__c),MONTH(MT_MediaBuildingBlock_ref__r.MT_MediaCampaign_ref__r.MT_Sys_ContractOrTVRuntimeEnd__c),1) &lt; DATE(YEAR(MT_PlanningDate__c),MONTH(MT_PlanningDate__c),1)
)</errorConditionFormula>
        <errorDisplayField>MT_PlanningDate__c</errorDisplayField>
        <errorMessage>Der Monat befindet sich au√üerhalb des Vertragszeitraumes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_Monthly_OverbookingFlights_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>MT_NoOverbookingFlights__c &gt; 5</errorConditionFormula>
        <errorMessage>Only 5 Overbooking Flights can be created per month</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_Monthly_Probability_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>MT_Probability__c &lt; 0 || MT_Probability__c &gt; 1</errorConditionFormula>
        <errorDisplayField>MT_Probability__c</errorDisplayField>
        <errorMessage>Value must be between 0% and 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_Monthly_ROIYield_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>MT_ROI_Yield__c &lt; 0</errorConditionFormula>
        <errorDisplayField>MT_ROI_Yield__c</errorDisplayField>
        <errorMessage>Value cannot be lower than 0%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_Monthly_SponsoringFlights_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>MT_NoSponsoringFlights__c &gt; 20</errorConditionFormula>
        <errorMessage>Only 20 Sponsoring Flights can be created per month</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MT_Monthly_SpotFlights_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>MT_NoSpotFlights__c &gt; 5</errorConditionFormula>
        <errorMessage>Only 5 Spot Flights can be created per month</errorMessage>
    </validationRules>
