    <validationRules>
        <fullName>TRIMDA__Invalid_License_Number</fullName>
        <active>true</active>
        <errorConditionFormula>OR(  TRIMDA__LicenseNo__c  = &quot;000000000000000000&quot;, LEN(TRIMDA__LicenseNo__c ) &lt; 18 )</errorConditionFormula>
        <errorDisplayField>TRIMDA__LicenseNo__c</errorDisplayField>
        <errorMessage>Invalid License Number, enter your License number, or contact Datatrim to get registered (http://www.datatrim.com)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TRIMDA__License_Type_Validation</fullName>
        <active>true</active>
        <description>Validates the License type correspondence with the License No</description>
        <errorConditionFormula>CASE(TRIMDA__License_Type__c,
&quot;Trial&quot;, IF(LEFT(TRIMDA__LicenseNo__c,2)=&quot;00&quot;,1,0),
&quot;Basic&quot;, IF(LEFT(TRIMDA__LicenseNo__c,2)=&quot;33&quot;,1,0),
&quot;Standard&quot;, IF(LEFT(TRIMDA__LicenseNo__c,2)=&quot;11&quot;,1,0),
&quot;Enterprise&quot;, IF(LEFT(TRIMDA__LicenseNo__c,2)=&quot;22&quot;,1,0),
&quot;Custom&quot;, IF(LEFT(TRIMDA__LicenseNo__c,2)=&quot;99&quot;,1,0),
0
)=0</errorConditionFormula>
        <errorDisplayField>TRIMDA__License_Type__c</errorDisplayField>
        <errorMessage>This License Type value is not allowed for the current License, Contact d2b International to upgrade your license.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TRIMDA__Master_equalsSecondary_not_valid</fullName>
        <active>true</active>
        <description>Master=Secondary not valid for Lead to Account and lead to contact deduplications</description>
        <errorConditionFormula>AND( OR( ISPICKVAL( TRIMDA__Dupe_Alert_Type__c , &quot;Lead-Account&quot;) ,
ISPICKVAL( TRIMDA__Dupe_Alert_Type__c , &quot;Lead-Contact&quot;)) , TRIMDA__MasterEqualsSecondary__c = true)</errorConditionFormula>
        <errorDisplayField>TRIMDA__MasterEqualsSecondary__c</errorDisplayField>
        <errorMessage>You cannot select the Master=Secondary check box for Lead-Account or Lead-Contact deduplications. For these Alert Types the Leads are the secondary data and the Account/Contact are the Master data</errorMessage>
    </validationRules>
