    <fields>
        <fullName>TRIMDA__Alert_Status__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( TRIMDA__Active__c  = false, &quot;Alert is not Active&quot;, 
CASE( TRIMDA__Run_Frequency__c ,&quot;Once&quot;,
IF( TRIMDA__Last_Run_Date__c &gt;= TODAY(),&quot;Not Scheduled, Reference for New and changed Records must be in the Past&quot;,
CASE( TRIMDA__TimeSlot__c ,&quot;Next&quot;,IF(  ISPICKVAL(TRIMDA__Run_Status__c,&apos;Running, Please Wait...&apos; ),&quot;Running&quot;,&quot;Alert is scheduled to run in Next timeslot&quot;),&quot;Alert is scheduled to run based on Timeslot setting&quot;)), &quot;Recuring Alert Active and Scheduled &quot; ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Alert Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
