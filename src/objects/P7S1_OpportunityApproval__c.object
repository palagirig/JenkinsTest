    <validationRules>
        <fullName>P7S1_BeschreibungPflichtRahmenfreigabe</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISPICKVAL(P7S1_CCOFreigabe__c , &quot;Rahmenfreigabe&quot;) ,    ISBLANK(P7S1_BeschreibungCCORahmenfreigabe__c))</errorConditionFormula>
        <errorDisplayField>P7S1_BeschreibungCCORahmenfreigabe__c</errorDisplayField>
        <errorMessage>Rahmenfreigabe: bitte aus dem zur Verfügung stehenden Excel Sheet die entsprechende qualitative Rahmenfreigabe in die Beschreibung kopieren</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>P7S1_BeschreibungPflichtRahmenfreigabeMB</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( (P7S1_RahmenfreigabeMediaboard__c  = true) ,    ISBLANK( P7S1_BeschreibungRahmenfreigabeMB__c ))</errorConditionFormula>
        <errorDisplayField>P7S1_BeschreibungRahmenfreigabeMB__c</errorDisplayField>
        <errorMessage>Rahmenfreigabe: bitte aus dem zur Verfügung stehenden Excel Sheet die entsprechende qualitative Rahmenfreigabe in die Beschreibung kopieren</errorMessage>
