    <validationRules>
        <fullName>Datum_Bis_nach_Datum_Von</fullName>
        <active>true</active>
        <description>Das Datum bis muss nach dem Datum von liegen.</description>
        <errorConditionFormula>GueltigVon__c &gt; GueltigBis__c</errorConditionFormula>
        <errorMessage>Bitte ein gültiges Datum nach dem Datum von eingeben.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SomTv_MehrAlsEineRelevanteKABeziehung</fullName>
        <active>true</active>
        <description>Überprüft auf Basis des Feldes Anzahl KA Beziehungen Analyse, ob mehr als eine relevante K-A Beziehung vorhanden ist</description>
        <errorConditionFormula>IF (
ISNEW() = TRUE  &amp;&amp;  SomTv_RelevanteBeziehung__c = TRUE &amp;&amp; Kunde__r.P7S1_AnzahlKABeziehungenRelevant__c = 1, TRUE,
  IF(ISNEW() = FALSE &amp;&amp;  ISCHANGED(SomTv_RelevanteBeziehung__c) = True &amp;&amp; SomTv_RelevanteBeziehung__c = True &amp;&amp; Kunde__r.P7S1_AnzahlKABeziehungenRelevant__c = 1, TRUE, FALSE)
   )</errorConditionFormula>
        <errorDisplayField>SomTv_RelevanteBeziehung__c</errorDisplayField>
        <errorMessage>&quot;Eine relevante Kundenagentur Beziehung ist bereits vorhanden. Es darf nur eine relevante K-A-Beziehung angelegt sein&quot;</errorMessage>
    </validationRules>
