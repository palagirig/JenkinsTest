    <validationRules>
        <fullName>SomDig_VertriebsteamDigital</fullName>
        <active>true</active>
        <description>Wenn es sich beim Benutzer um einen Sales-Mitarbeiter der SevenOne Media Digital handelt, muss aus Reportinggründen immer das Feld &apos;Vertriebsteam Digital&apos; gepflegt sein</description>
        <errorConditionFormula>AND(
IsActive,  
ProfileId = &quot;00e20000001QBtU&quot;,  
ISBLANK(Text(SomDig_SalesTeam__c)), 
Or(CONTAINS(Department,&quot;Sales&quot;), 
CONTAINS(Department,&quot;Yield&quot;), 
CONTAINS(Department,&quot;ECC&quot;), 
CONTAINS(Department,&quot;Entertainment&quot;)), 
NOT(ISPICKVAL(P7S1_UserType__c, &quot;System_Tester&quot;))  
)</errorConditionFormula>
        <errorDisplayField>SomDig_SalesTeam__c</errorDisplayField>
        <errorMessage>Es handelt sich beim Benutzer um einen Sales-Mitarbeiter der SevenOne Media Digital; hier bitte immer das Feld &apos;Vertriebsteam Digital&apos; pflegen</errorMessage>
    </validationRules>
